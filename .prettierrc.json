{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "^react(.*)$",
    "^mobx(.*)$",
    "^@cere(.*)$",
    "^@(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^~/",
    "^[./]",
    "^[../]"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderSeparation": false,
  "importOrderGroupNamespaceSpecifiers": true
}
