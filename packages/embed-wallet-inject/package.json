{
  "name": "@cere/embed-wallet-inject",
  "version": "0.12.1",
  "sideEffects": false,
  "types": "./src",
  "main": "./dist/cjs",
  "exports": {
    "import": "./dist/esm",
    "require": "./dist/cjs"
  },
  "dependencies": {
    "@polkadot/extension-inject": "^0.45.4"
  },
  "peerDependencies": {
    "@polkadot/types": "^10.2.1",
    "@cere/embed-wallet": "*"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project ./tsconfig.json",
    "build:cjs": "tsc --project ./tsconfig.cjs.json"
  }
}
